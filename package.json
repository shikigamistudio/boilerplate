{
  "name": "@shikigamistudio/boilerplate",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node bin/server.js",
    "prebuild": "npm run format && npm run lint:fix",
    "build": "node ace build",
    "predev": "npm run docker:start",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose down",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#abilities/*": "./app/abilities/*.js",
    "#actions/*": "./app/actions/*.js",
    "#config/*": "./config/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#database/*": "./database/*.js",
    "#events/*": "./app/events/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#helpers/*": "./app/helpers/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#mails/*": "./app/mails/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#models/*": "./app/models/*.js",
    "#policies/*": "./app/policies/*.js",
    "#providers/*": "./providers/*.js",
    "#rules/*": "./app/rules/*.js",
    "#services/*": "./app/services/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#types/*": "./types/*.js",
    "#validators/*": "./app/validators/*.js"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^2.0.1",
    "@adonisjs/lucid": "^21.3.0",
    "@adonisjs/mail": "^9.2.2",
    "@adonisjs/session": "^7.5.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^3.0.0",
    "@inertiajs/react": "^1.2.0",
    "@vinejs/vine": "^2.1.0",
    "edge.js": "^6.2.0",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@ayato-san/adonis-commands": "^1.2.2",
    "@ayato-san/tailwind-plugin": "^2.1.1",
    "@ayato-san/tooling-configs": "^2.0.4",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.40",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "hot-hook": "^0.3.1",
    "pino-pretty": "^11.3.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-edgejs": "^0.2.32",
    "tailwindcss": "^3.4.14",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@ayato-san/tooling-configs/prettier/edgejs"
}
